{
  "$schema": "http://json.schemastore.org/template",
  "author": "Microsoft",
  "classifications": [ "Universal" ],
  "name": "C# Markup Code Style",
  "shortName": "C# Markup",
  "groupIdentity": "ts.WinUI.Feat.CodingStyleMarkupService",
  "identity": "ts.WinUI.Feat.CodingStyleMarkupService",
  "description": "Adds C# Markup Code Style to your application.",
  "tags": {
    "language": "C#",
    "type": "project",
    "ts.type": "CODINGSTYLE",
    "ts.projecttype": "all",
    "ts.frontendframework": "all",
    "ts.platform": "WinUI",
    "ts.appmodel": "Desktop",
    "ts.version": "1.0.0",
    "ts.genGroup": "0",
    "ts.group": "CodeStyleInteraction",
    "ts.displayOrder": "1",
    "ts.multipleInstance": "false",
    "ts.rightClickEnabled": "true"
  },

  "sourceName": "Param_ItemName",
  "preferNameDirectory": true,
  "PrimaryOutputs": [
    {
      "path": "Contracts/Services/IFeaturePWAService.cs"
    },
    {
      "path": "Services/FeaturePWAService.cs"
    }
  ],
  "symbols": {
    "ts.rootNamespace": {
      "type": "parameter",
      "replaces": "Param_RootNamespace"
    }
  }
}
