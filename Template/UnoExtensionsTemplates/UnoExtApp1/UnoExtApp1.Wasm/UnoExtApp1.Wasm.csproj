<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <NoWarn>$(NoWarn);NU1504;NU1505;NU1701</NoWarn>
    <!-- Disabled due to issue with Central Package Management with implicit using -->
    <ImplicitUsings>disable</ImplicitUsings>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <MonoRuntimeDebuggerEnabled>true</MonoRuntimeDebuggerEnabled>
    <DefineConstants>$(DefineConstants);TRACE;DEBUG</DefineConstants>
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <!--
		IL Linking is disabled in Debug configuration.
		When building in Release, see https://platform.uno/docs/articles/features/using-il-linker-webassembly.html
		-->
    <WasmShellILLinkerEnabled>false</WasmShellILLinkerEnabled>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Debug' or '$(IsUiAutomationMappingEnabled)'=='True'">
    <IsUiAutomationMappingEnabled>True</IsUiAutomationMappingEnabled>
    <DefineConstants>$(DefineConstants);USE_UITESTS</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="Assets\SplashScreen.png" />
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Include="..\UnoExtApp1.UI\**\*.xaml" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="WasmCSS\Fonts.css" />
    <EmbeddedResource Include="WasmScripts\AppManifest.js" />
  </ItemGroup>
  <ItemGroup>
    <LinkerDescriptor Include="LinkerConfig.xml" />
  </ItemGroup>
  <ItemGroup>
    <!--
		This item group is required by the project template because of the
		new SDK-Style project, otherwise some files are not added automatically.

		You can safely remove this ItemGroup completely.
		-->
    <None Include="Program.cs" />
    <None Include="LinkerConfig.xml" />
    <None Include="wwwroot\web.config" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="6.0.0" />
    <PackageReference Include="Uno.Extensions.Logging.WebAssembly.Console" Version="1.4.0" />
    <PackageReference Include="Uno.Wasm.Bootstrap" Version="3.3.1" />
    <PackageReference Include="Uno.Wasm.Bootstrap.DevServer" Version="3.3.1" />
    <PackageReference Include="Uno.WinUI.WebAssembly" Version="4.6.19" />
    <PackageReference Include="Uno.WinUI.RemoteControl" Version="4.6.19" Condition="'$(Configuration)'=='Debug'" />
    <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="4.6.19" />
    <PackageReference Include="Uno.Material.WinUI" Version="2.3.0" />
    <PackageReference Include="Uno.Toolkit.WinUI.Material" Version="2.4.0-dev.114" />
    <PackageReference Include="Uno.Toolkit.WinUI" Version="2.4.0-dev.114" />
    <PackageReference Include="Uno.Extensions.Authentication.WinUI" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Hosting.WinUI" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Localization.WinUI" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Logging.WinUI" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Navigation.Toolkit.WinUI" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Navigation.WinUI" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Authentication" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Configuration" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Core" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Hosting" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Http" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Http.Refit" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Localization" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Logging.Serilog" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Navigation" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Serialization" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Serialization.Http" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Serialization.Refit" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Reactive" Version="2.3.0-dev.343" />
    <PackageReference Include="Uno.Extensions.Reactive.WinUI" Version="2.3.0-dev.343" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\UnoExtApp1\UnoExtApp1.csproj" />
  </ItemGroup>
  <Import Project="..\UnoExtApp1.UI\UnoExtApp1.UI.projitems" Label="Shared" Condition="Exists('..\UnoExtApp1.UI\UnoExtApp1.UI.projitems')" />
</Project>