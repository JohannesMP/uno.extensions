<Project Sdk="Microsoft.NET.Sdk">
	<Import Project="..\tfms-ui-maui.props" />

	<PropertyGroup>
		<UseMaui>true</UseMaui>
		<RootNamespace>Uno.Extensions.Maui</RootNamespace>
		<NoWarn>$(NoWarn);NU5104;NU5048;NU1009</NoWarn>
		<Description>Core Extensions to embed .NET MAUI controls within your Uno app.</Description>
		<WarningsNotAsErrors>$(WarningsNotAsErrors);CS1591</WarningsNotAsErrors>
	</PropertyGroup>

	<ItemGroup>
		<InternalsVisibleTo Include="Uno.Extensions.Maui.WinUI" />
		<PackageReference Include="Microsoft.Extensions.Hosting" />
		<PackageReference Include="Microsoft.Extensions.Logging" />
	</ItemGroup>

</Project>
