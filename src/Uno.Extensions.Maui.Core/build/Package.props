<?xml version="1.0" encoding="utf-8" ?>
<Project>
  <Choose>
    <When Condition="$(TargetFramework.Contains('windows10'))">
      <PropertyGroup Condition=" '$(OutputType)' != 'WinExe' AND '$(OutputType)' != 'Exe' ">
        <EnableMsixTooling>false</EnableMsixTooling>
      </PropertyGroup>
    </When>
  </Choose>
</Project>
